<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>AttentionAI</title>
    <link rel="stylesheet" href="styles.css">
</head>
<body>
    <nav>
        <div class="brand-div" id="brand-div">
            <div class="logo-div">
                <img src="images/logo.png" alt="logo" id="logo">
            </div>
            <p id="logo-title">AttentionAI</p>
        </div>
        <button onclick="main()" id="notification-btn">Enable Notifications</button>
        <div class="user-div">
            <div class="profile-pic-div">
                <img src="images/user-profile-pic.png" alt="user-icon" class="user-icon" id="user-icon">
            </div>
            
        </div>
    </nav>
</body>
</html>

<!-- Script for Service Worker -->
<script src="js/register-sw.js"></script>
<script src="js/header.js"></script>

<!-- Script for profile picture. pic is provided by server -->
<script>
    const userIcon = document.getElementById("user-icon");
    const picLink = "<%= pic %>";
    document.addEventListener('DOMContentLoaded', (e) => {
    userIcon.setAttribute('src', picLink);
});
</script>